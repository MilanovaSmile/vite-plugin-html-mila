{
  "name": "vite-plugin-html-mila",
  "private": false,
  "version": "2.0.3",
  "description": "Vite plugin to copy and minify HTML",
  "author": "Milana Kustinskaya <milanovasmile@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/MilanovaSmile/vite-plugin-html-mila",
  "type": "module",
  "keywords": [
    "vite",
    "vite-plugin",
    "html",
    "compiler"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/hmtl-mila.umd.js",
  "module": "./dist/html-mila.es.js",
  "exports": {
    ".": {
      "import": "./dist/html-mila.es.js",
      "require": "./dist/html-mila.umd.js"
    }
  },
  "scripts": {
    "dev": "vite build --config dev.vite.config.js",
    "build": "vite build --config build.vite.config.js"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "html-minifier-terser": "^7.2.0"
  },
  "devDependencies": {
    "vite": "^5.3.3"
  },
  "peerDependencies": {}
}
